<%= form_with(url: "/users", method: "get") do %></br></br>
  <%= label_tag("username")%></br></br>
  <%= text_field_tag(:username, "Name")%></br></br>
  <%= label_tag(:email, "Email:")%></br></br>
  <%= email_field_tag(:email, nil, placeholder: "example@example.com")%></br></br>
  <%= label_tag(:password, "Password:")%></br></br>
  <%= password_field_tag(:password)%></br></br>
  <%= submit_tag("Submit")%>
<% end %>

